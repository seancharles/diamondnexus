<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="admin-login" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="logo">
            <arguments>
                <argument name="logo_image_src" xsi:type="string">images/fc-logo-mark-184x184.png</argument>
                <argument name="logo_width" xsi:type="number">184</argument> <!-- Add custom logo width -->
                <argument name="logo_height" xsi:type="number">184</argument> <!-- Add custom logo height -->
            </arguments>
        </referenceBlock>
    </body>
 </page>