{
    "name": "progressive/pay-with-progressive",
    "description": "Progressive Leasing ECommerce Payment gateway for Magento 2",
    "type": "magento2-module",
    "version": "2.4.0",
    "license": [],
    "require": {
        "php": "~5.6.5|7.0.2|7.0.4|~7.0.6",
        "magento/magento-composer-installer": "*",
        "magento/module-payment": "100.1.*",
        "magento/module-checkout": "100.1.*",
        "magento/framework": "100.1.*",
        "magento/module-sales": "100.1.*"
    },
    "extra": {
        "map": [
            [
                "*",
                "Progressive/PayWithProgressive"
            ]
        ]
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Progressive\\PayWithProgressive\\": ""
        }
    }
}
