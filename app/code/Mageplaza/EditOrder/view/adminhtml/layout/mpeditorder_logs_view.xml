<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="admin-1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="editor"/>
    <body>
        <referenceContainer name="content">
            <block class="Mageplaza\EditOrder\Block\Adminhtml\Logs\Button" name="mpeditorder_log_button"/>
            <block class="Mageplaza\EditOrder\Block\Adminhtml\Logs\View" name="mpeditorder_log_view" template="Mageplaza_EditOrder::logs/view.phtml">
                <block class="Mageplaza\EditOrder\Block\Adminhtml\Logs\View" name="info" template="Mageplaza_EditOrder::logs/order/info.phtml" />
                <block class="Mageplaza\EditOrder\Block\Adminhtml\Logs\Order\Account" name="account" template="Mageplaza_EditOrder::logs/order/account.phtml" />
                <block class="Mageplaza\EditOrder\Block\Adminhtml\Logs\View" name="billing_address" template="Mageplaza_EditOrder::logs/order/billing_address.phtml" />
                <block class="Mageplaza\EditOrder\Block\Adminhtml\Logs\View" name="shipping_address" template="Mageplaza_EditOrder::logs/order/shipping_address.phtml" />
                <block class="Mageplaza\EditOrder\Block\Adminhtml\Logs\Order\PaymentMethod" name="payment_method" template="Mageplaza_EditOrder::logs/order/payment_method.phtml" />
                <block class="Mageplaza\EditOrder\Block\Adminhtml\Logs\Order\ShippingMethod" name="shipping_method" template="Mageplaza_EditOrder::logs/order/shipping_method.phtml" />
                <block class="Mageplaza\EditOrder\Block\Adminhtml\Logs\Order\Items" name="items" template="Mageplaza_EditOrder::logs/order/items.phtml" />
                <block class="Mageplaza\EditOrder\Block\Adminhtml\Logs\Order\Total" name="total" template="Mageplaza_EditOrder::logs/order/total.phtml" />
            </block>
        </referenceContainer>
    </body>
</page>
